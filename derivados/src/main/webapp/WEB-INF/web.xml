<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>libreriaCesar</display-name>
	<servlet>
		<description></description>
		<servlet-name>GettingPaths</servlet-name>
		<servlet-class>mx.com.libreria.servlets.GettingPaths</servlet-class>
		<load-on-startup>4</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>ServletDemo2ChartGenerator</servlet-name>
		<servlet-class>mx.com.libreria.servlets.ServletDemo2ChartGenerator</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletBono</servlet-name>
		<servlet-class>mx.com.libreria.servlets.ServletBono</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletSwap</servlet-name>
		<servlet-class>mx.com.libreria.servlets.ServletSwap</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ServletDemo2ChartGenerator</servlet-name>
		<url-pattern>/servlet/ServletDemo2ChartGenerator</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletBono</servlet-name>
		<url-pattern>/servlet/ServletBono</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletSwap</servlet-name>
		<url-pattern>/servlet/ServletSwap</url-pattern>
	</servlet-mapping>

	<session-config>
		<session-timeout>-1</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>login/index.jsp</welcome-file>
	</welcome-file-list>
</web-app>